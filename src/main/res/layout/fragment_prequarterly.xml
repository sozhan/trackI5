<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <RelativeLayout
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/student_name"
            android:text="RollNo: 1101"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/student_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/student_information"
            android:text="Pre Quarterly Student info"
            style="?android:listSeparatorTextViewStyle"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="@android:color/holo_blue_dark"
            />
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/student_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/student_name_heading"
            android:text="Name :"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@+id/student_name_heading"
            android:layout_toEndOf="@+id/student_name_heading"
            android:layout_below="@+id/student_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/student_name_value"
            android:text="Kevin John"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/tab_indicator_text"
            />
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/student_name_heading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/parent_name_heading"
            android:text="Parents :"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginLeft="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/parent_names"
            android:layout_toEndOf="@+id/parent_name_heading"
            android:layout_toRightOf="@+id/parent_name_heading"
            android:textColor="@android:color/tab_indicator_text"
            android:text="Dave and Jane Doe"
            android:textSize="15dp"
            android:textStyle="bold"
            android:layout_below="@+id/student_name_value" />
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/parent_names"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/classTeacher"
            android:text="Class Teacher :"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginLeft="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Teacher_name"
            android:layout_toEndOf="@+id/classTeacher"
            android:layout_toRightOf="@+id/classTeacher"
            android:textColor="@android:color/tab_indicator_text"
            android:text="George D"
            android:textSize="15dp"
            android:textStyle="bold"
            android:layout_below="@+id/parent_names" />
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/classTeacher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/attendanceReport"
            style="?android:listSeparatorTextViewStyle"
            android:text="Attendance Report"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="@android:color/holo_blue_dark"
            />
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/attendanceReport"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/classPercentageAverage"
            android:text="Class Percentage Average: 94 %"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/classPercentageAverage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/studentPresentPercentage"
            android:text="Student Present: 86 %"
            android:textSize="15dp"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/studentPresentPercentage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/absentDays"
            android:text="No. of Absent days: 6 Days"
            android:textSize="15dp"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/absentDays"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/academic_report"
            android:text="Academic Report"
            style="?android:listSeparatorTextViewStyle"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="@android:color/holo_blue_dark"
            />
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/academic_report"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/overallGrade"
            android:text="OverAll Grade/% : 94%"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />
        <TextView
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/overallGrade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Rank"
            android:text="Rank : 12"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            />

        <TextView
            android:layout_marginTop="15dp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/overallGrade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/academic_record_information"
            android:text="academic record Information"
            style="?android:listSeparatorTextViewStyle"
            android:textSize="20dp"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:textColor="@android:color/holo_blue_dark"
            />

        <RelativeLayout
            android:layout_below="@+id/academic_record_information"
            android:layout_marginTop="10dp"
            android:id="@+id/first_table_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableLayout
                android:tag="Exams/test"
                android:background="@color/TableBg"
                android:layout_width="match_parent"
                android:shrinkColumns="*"
                android:stretchColumns="*"
                android:layout_height="wrap_content">
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="1dp" />
                <TableRow
                    android:id="@+id/tableRow"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:id="@+id/textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Course"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:id="@+id/textview_grade"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Grade"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />

                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Computer Grs"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="88"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Health (S)"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="P"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="PE 9(s)"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="92"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow4"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Eng 9"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="83"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow5"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="History Am I"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="90"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow6"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Algebra I"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="92"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>

                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow7"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="Biology"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="90"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow9"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="PFL II"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="90"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />



                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />


                <TableRow
                    android:id="@+id/tableRow8"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="French III"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:textSize="18dp"
                        android:text="87"
                        android:layout_marginLeft="15dp"
                        android:layout_span="6"
                        />
                </TableRow>
                <View android:layout_width="match_parent"
                    android:background="@android:color/white"
                    android:layout_height="2dp" />

            </TableLayout>


        </RelativeLayout>

        <Button
            android:layout_marginTop="30dp"
            android:layout_below="@+id/first_table_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/comments"
            android:text="Performance Comments: Good at Painting, Suggest to join for painting course"
            android:textSize="15dp"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:background="@color/themeColor"
            android:textColor="@android:color/white"
            />
    </RelativeLayout>
</ScrollView>