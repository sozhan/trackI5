<?xml version="1.0" encoding="utf-8"?>

<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="270"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="270">
    <shape
        android:innerRadiusRatio="5"
        android:shape="ring"
        android:thicknessRatio="8.0">
        <gradient
            android:centerColor="@color/material_green_600"
            android:endColor="@color/material_green_900"
            android:startColor="@color/material_green_300"
            android:angle="0"
            android:type="sweep"
            android:useLevel="false" />
    </shape>
</rotate>